class Solution {
        private int convertstringtonum(String s)
        {
            int result=0;
            for(int i=0;i<s.length();i++)
            {
                result=(s.charAt(i)-'a')+result*10;
            }
            return result;
        }
    public boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
  
        
        
   
          
    int p=convertstringtonum(firstWord);
    int q=convertstringtonum(secondWord);
    int r=convertstringtonum(targetWord);
        
    int w=p+q;
        
        if(w==r)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
}
